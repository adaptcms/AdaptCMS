{
    "name": "adaptcms/plugin-sitemap",
    "description": "Sitemap generator plugin for AdaptCMS.",
    "require": {
      	"php": ">=7.1.3",
        "watson/sitemap": "^2.0.21"
    },
    "extra": {
	    "laravel": {
	        "providers": [
	            "Watson\\Sitemap\\SitemapServiceProvider"
	        ],
	        "aliases": {
	            "Sitemap": "Watson\\Sitemap\\Facades\\Sitemap"
	        }
	    }
	}
}
